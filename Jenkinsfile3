//module 4 block 1
pipeline {
    agent any

    options {
        timeout(time: 1, unit: 'HOURS')
        disableConcurrentBuilds()
    }
    
    
    stages {
        stage('echo') {
            steps {
                script{
                    echo 'Build from DEV branch'
                    }
                }
            }
        }
    }

    post {
        always {
            deleteDir()
        }
    }
}
